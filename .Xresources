! Xft settings ---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! xterm ----------------------------------------------------------------------

!xterm*faceName:           Terminus:style=Regular:size=8
xterm*dynamicColors:      true
xterm*saveLines:          4096
xterm*scrollKey:          true
xterm*scrollTtyOutput:    false
xterm*scrollBar:          false
xterm*rightScrollBar:     false
xterm*jumpScroll:         true
xterm*multiScroll:        true
xterm*utf8:               1
xterm*eightBitInput:      true
xterm*background:	black
xterm*foreground:	white
xterm*termName:		xterm-256color
xterm*locale: 		true
XTerm*visualbell: true
xterm*bellIsUrgent: 	true
xterm*borderWidth: 	0

XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48
XTerm*on3Clicks: regex ([[:alpha:]]+://)?([[:alnum:]!#+,./=?@_~-]|(%[[:xdigit:]][[:xdigit:]]))+


XTerm*font:  -misc-fixed-medium-r-normal--10-120-75-75-C-70-iso10646-1
XTerm*font1: -*-terminus-medium-*-*-*-12-*-*-*-*-*-iso10646-*
XTerm*font2: -*-terminus-medium-*-*-*-14-*-*-*-*-*-iso10646-*
XTerm*font3: -*-terminus-medium-*-*-*-16-*-*-*-*-*-iso10646-*
XTerm*font4: -*-terminus-medium-*-*-*-22-*-*-*-*-*-iso10646-*
XTerm*font5: -*-terminus-medium-*-*-*-24-*-*-*-*-*-iso10646-*
XTerm*font6: -*-terminus-medium-*-*-*-32-*-*-*-*-*-iso10646-*

*font:	-*-fixed-medium-r-*-*-13-*-*-*-*-*-*-*

! Forbid bold font faces; bold type is light blue.
!XTerm*colorBDMode: true
!XTerm*colorBD: rgb:82/a4/d3
! Do not underscore text, underlined text is white.
!XTerm*colorULMode: true
!XTerm*colorUL: rgb:e4/e4/e4
